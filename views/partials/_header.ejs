<% if(!username){ %>
<h1 class="cent">Tiny Urls At Your Fingertips</h1>
<h3 class="cent">Login</h3>
<div class="textC">
  <div class="in-block">
    <form class='bstInline' method="POST">
      <div class="form-group max">
        <label for="exampleInputEmail1">Email address</label>
        <input name='username' type="email" class="form-control" aria-describedby="emailHelp" placeholder="Enter email"
          required>
        <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
      </div>
      <div class="form-group max">
        <label for="exampleInputPassword1">Password</label>
        <input name='password' type="password" class="form-control" placeholder="Password" required>
      </div>
      <button type="submit" class="btn btn-success" formaction="/login">Login</button>
      <button type='submit' class="btn btn-info linkReset" formaction="/register">Register</button>
    </form>
  </div>
</div>
<% }else{ %>
<% } %>